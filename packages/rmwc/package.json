    "@rmwc/tabs": "workspace:*",
    "@rmwc/textfield": "workspace:*",
    "@rmwc/theme": "workspace:*",
    "@rmwc/toggleable": "workspace:*",
    "@rmwc/tooltip": "workspace:*",
    "@rmwc/rc-tooltip": "workspace:*",
    "@rmwc/top-app-bar": "workspace:*",
    "@rmwc/types": "workspace:*",
    "@rmwc/typography": "workspace:*",
    "material-components-web": "^14.0.0"
  }

  "scripts": {
    "start": "nx serve docs & nx watch --projects=readme -- npm run docgen:examples && wait -n",
    "build:docs": "nx build docs",
    "build:components": "nx run-many -t build --projects=tag:publishable",
    "docgen:markdown": "node --loader=./scripts/docgen-markdown-loader.mjs scripts/docgen-markdown.mjs",
    "docgen:examples": "node scripts/docgen-examples.js",
    "docgen:props": "node scripts/docgen-props.mjs",
    "test": "nx run-many -t test",
    "release:patch": "nx version workspace --releaseAs=patch",
    "release:minor": "nx version workspace --releaseAs=minor",
    "release:major": "nx version workspace --releaseAs=major",
    "release:alpha": "nx version workspace --releaseAs=prerelease --preid=alpha",
    "prerelease:patch": "nx version workspace --releaseAs=prepatch --preid=alpha",
    "prerelease:minor": "nx version workspace --releaseAs=preminor --preid=alpha",
    "prerelease:major": "nx version workspace --releaseAs=premajor --preid=alpha",
    "prerelease:alpha": "nx version workspace --releaseAs=prerelease --preid=alpha",
    "prettier": "nx format:write",
    "publish:packages": "nx run-many -t build --projects=tag:publishable && node ./scripts/fix-dependency-versions.js && nx run-many -t npm-deploy --projects=tag:publishable",
    "publish:docs": "npm run build:docs && git add docs -f && git commit --allow-empty -m 'Deploying docs'",
    "changelog": "changelog || echo 'No Changes'",
    "prepare": "husky install",
    "validate:actions": "node scripts/validate-actions.mjs"
  },

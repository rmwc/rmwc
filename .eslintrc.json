{
  "extends": "react",
  "plugins": ["react", "jsx-a11y", "import"],
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true,
    "jasmine": true
  },
  "globals": {
    "React": true
  },
  "rules": {
    "space-before-function-paren": "off",
    "indent": ["error", 2],
    "max-len": [
      "error",
      80,
      {
        "ignoreComments": true,
        "ignoreTrailingComments": true,
        "ignoreUrls": true
      }
    ],
    "react/prop-types": "off",
    "no-unused-expressions": "off"
  }
}
